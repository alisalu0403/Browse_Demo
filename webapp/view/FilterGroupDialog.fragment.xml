<core:FragmentDefinition
		xmlns="sap.m"
		xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		confirm="handleConfirm">
		<filterItems>
			<ViewSettingsFilterItem text="{i18n>filterItemHeader}" key="Orders" multiSelect="false">
				<items>
					<ViewSettingsItem text="{i18n>filterItemTextShip}" key="shiporder" />
					<ViewSettingsItem text="{i18n>filterItemTextNoShip}" key="shipment" />
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
		<groupItems>
			<ViewSettingsItem text="{i18n>groupItemCustomer}" key="CompanyName" />
			<ViewSettingsItem text="{i18n>groupItemOrder}" key="OrderDate" />
			<ViewSettingsItem text="{i18n>groupItemShip}" key="ShippedDate" />
		</groupItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>